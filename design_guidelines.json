{
  "project_name": "TutorPlatform_MVP",
  "design_philosophy": {
    "theme": "Organic Growth",
    "archetype": "Organic & Earthy",
    "vibe": "Nurturing, Academic, Modern, Trustworthy",
    "core_values": ["Clarity", "Growth", "Connection"]
  },
  "typography": {
    "font_family": {
      "heading": "Outfit, sans-serif",
      "body": "DM Sans, sans-serif",
      "accent": "Caveat, cursive"
    },
    "scale": {
      "h1": "text-5xl md:text-7xl font-bold tracking-tight",
      "h2": "text-4xl md:text-5xl font-semibold tracking-tight",
      "h3": "text-2xl md:text-3xl font-medium",
      "body_lg": "text-lg md:text-xl leading-relaxed",
      "body": "text-base leading-relaxed text-muted-foreground"
    }
  },
  "colors": {
    "palette": {
      "background": "#F9F8F6",
      "foreground": "#2C3333",
      "primary": "#2F5D62",
      "primary_foreground": "#FFFFFF",
      "secondary": "#DF7861",
      "secondary_foreground": "#FFFFFF",
      "accent": "#ECB390",
      "accent_foreground": "#2C3333",
      "muted": "#ECEBE4",
      "muted_foreground": "#6B7280",
      "card": "#FFFFFF",
      "card_foreground": "#2C3333",
      "border": "#E2E0D6"
    },
    "usage": {
      "body_bg": "bg-[#F9F8F6]",
      "text_main": "text-[#2C3333]",
      "text_muted": "text-[#6B7280]",
      "brand_primary": "bg-[#2F5D62] text-white hover:bg-[#23464A]",
      "brand_secondary": "bg-[#DF7861] text-white hover:bg-[#C6604B]",
      "glass_surface": "bg-white/60 backdrop-blur-xl border border-white/40 shadow-sm"
    }
  },
  "layout_strategy": {
    "container": "container mx-auto px-4 md:px-6 lg:px-8 max-w-7xl",
    "spacing": {
      "section_gap": "py-24 md:py-32",
      "element_gap": "gap-8 md:gap-12"
    },
    "grid_system": {
      "bento_hero": "grid grid-cols-1 md:grid-cols-12 gap-6",
      "features": "grid grid-cols-1 md:grid-cols-3 gap-8",
      "testimonials": "grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6"
    }
  },
  "visual_enhancers": {
    "radius": "rounded-2xl",
    "shadows": {
      "soft": "shadow-[0_8px_30px_rgb(0,0,0,0.04)]",
      "hover": "hover:shadow-[0_8px_30px_rgb(0,0,0,0.12)] hover:-translate-y-1 transition-all duration-300"
    },
    "textures": "bg-[url('https://www.transparenttextures.com/patterns/cubes.png')] opacity-5",
    "decorations": [
      "Use handwritten annotations (SVG) near key headings",
      "Soft gradient blobs in background (primary/accent colors) with blur-3xl"
    ]
  },
  "components": {
    "Hero": {
      "layout": "Asymmetrical Split",
      "left": "H1 Heading + Value Prop + CTA + Social Proof",
      "right": "Image Grid (Masonry) or Floating 3D Elements",
      "details": "Use 'Caveat' font for a 'Join 1000+ students' note with an arrow."
    },
    "Subjects": {
      "layout": "Bento Grid",
      "style": "Cards with colored light backgrounds (e.g., bg-blue-50) and large icons.",
      "interaction": "Hover scale effect"
    },
    "HowItWorks": {
      "layout": "Zig-zag or Step Process",
      "connector": "Dashed line connecting steps (SVG path)"
    },
    "Teachers": {
      "layout": "Horizontal Scroll / Carousel",
      "card_style": "Portrait photo top, name/subject bottom. Clean white card with shadow."
    },
    "DemoModal": {
      "component": "Shadcn Dialog",
      "form": "Shadcn Form + Zod Validation",
      "fields": ["Name", "Email", "Grade/Level", "Subject Interest", "Preferred Date"],
      "style": "Clean, single column. Success state shows confetti."
    },
    "AdminPanel": {
      "style": "Sidebar layout. Simple data tables using Shadcn Table. Charts for 'Visitors vs Bookings'."
    }
  },
  "image_urls": {
    "hero_main": "https://images.pexels.com/photos/5905709/pexels-photo-5905709.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940",
    "hero_secondary": "https://images.unsplash.com/photo-1623485101793-082c03565fa9?crop=entropy&cs=srgb&fm=jpg&q=85",
    "teachers": [
      "https://images.unsplash.com/photo-1758685848001-0396a85ba84f?crop=entropy&cs=srgb&fm=jpg&q=85",
      "https://images.unsplash.com/photo-1696960181436-1b6d9576354e?crop=entropy&cs=srgb&fm=jpg&q=85",
      "https://images.unsplash.com/photo-1758685845906-6f705cde4fb7?crop=entropy&cs=srgb&fm=jpg&q=85",
      "https://images.unsplash.com/photo-1758685847747-597ce085906e?crop=entropy&cs=srgb&fm=jpg&q=85"
    ],
    "background_shapes": [
      "https://images.unsplash.com/photo-1739055248868-6a763d2e63aa?crop=entropy&cs=srgb&fm=jpg&q=85",
      "https://images.unsplash.com/photo-1768815210987-591340ed69fb?crop=entropy&cs=srgb&fm=jpg&q=85"
    ],
    "testimonials": [
        "https://images.unsplash.com/photo-1655720348616-184ae7fad7e3?crop=entropy&cs=srgb&fm=jpg&q=85",
        "https://images.pexels.com/photos/8199247/pexels-photo-8199247.jpeg?auto=compress&cs=tinysrgb&dpr=2&h=650&w=940"
    ]
  },
  "universal_guidelines": [
    "Do not use generic 'Lorem Ipsum'. Use realistic copy related to education.",
    "Ensure 3:1 contrast ratio for all text.",
    "Use 'lucide-react' for icons.",
    "Animations must be respectful of 'prefers-reduced-motion'.",
    "Mobile padding should be p-4 or p-6. Desktop p-8 or p-12.",
    "Buttons must have a specialized hover state (lift + shadow)."
  ],
  "instructions_to_main_agent": [
    "Install 'lenis' for smooth scrolling.",
    "Install 'framer-motion' for animations.",
    "Update tailwind.config.js to include the new color palette and fonts.",
    "Create a 'components/layout' folder for semantic sections (Hero, Features, etc.).",
    "Use 'react-hook-form' and 'zod' for the Booking Modal.",
    "Mock the video functionality with a placeholder image and a 'Play' button overlay."
  ]
}